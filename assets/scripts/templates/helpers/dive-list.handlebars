{{#each divelogs as |divelog|}}
  {{log divelog}}
  <!-- {{> partial}}
  Don't uncomment yet -->

  <div>
    <h1 data-id="{{divelog.id}}"> <small>Country: {{divelog.country}}</small></h1>
    <section data-id="{{divelog.id}}">
      <p>{{#if divelog.user}}
